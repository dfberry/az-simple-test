{
  "root": true,
  "plugins": ["prettier"],
  "env": {
    "node": true,
    "browser": false,
    "commonjs": true,
    "es6": true
  },
  "extends": ["eslint:recommended", "prettier"],
  "rules": {
    "no-console": 0, 
    "prettier/prettier": 2, 
    "no-implicit-globals":"error"
  },
  "parserOptions": {
    "ecmaVersion": "latest"
}
}
